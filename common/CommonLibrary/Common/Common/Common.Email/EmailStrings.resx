                    &lt;img src="cid:{HeaderImage}" width="632" height="30" /&gt;
            &lt;/tr&gt;
            &lt;tr&gt;
                &lt;td&gt;
                    &lt;div style='margin: 40px 40px 25px 40px; font-size:14px; letter-spacing: 0.6px'&gt;
                        &lt;p style="margin-bottom:30px"&gt;Hi {FirstName},&lt;/p&gt;


                        &lt;p style="margin-bottom:20px"&gt;There’s a new application waiting for your review on Microsoft&lt;br/&gt;Project “Odin”.&lt;/p&gt;
                        &lt;p style="margin-bottom:20px"&gt;Sign up now to review your new {PositionTitle} candidate.&lt;/p&gt;

                        &lt;p&gt;&lt;br&gt;&lt;/p&gt;
                        
                        &lt;table width="180px" border="0" cellspacing="0" cellpadding="0"&gt;
                          &lt;tr&gt;
                            &lt;td&gt;
                              &lt;table border="0" cellspacing="0" cellpadding="0"&gt;
