  <data name="EmailMessageTemplate_ManagerInvite" xml:space="preserve">
    <value>&lt;html&gt;
&lt;body style="font-family:Arial"&gt;
    &lt;table style="width:575px; border-color:#C7C7C7; border-width:1px; border-spacing:0px; border-style:solid;"&gt;
        &lt;tbody&gt;
            &lt;tr style="margin:0;padding:0"&gt;
